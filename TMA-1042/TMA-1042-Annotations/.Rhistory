colnames(HE_part_stat) <- c('TMA.core', 'H&E', 'P16', 'P53', 'P63', 'uv-GATA3', 'Ki67', 'CK20', 'CK5-6', 'Her2Neu', 'Cyclin')
View(fit_cluster_kmeans)
